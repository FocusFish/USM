<TableView class="oracle.dbtools.crest.model.design.relational.TableView" directorySegmentName="seg_0" id="4F4CE669-7E05-38A7-66E7-7781F85E797D" schemaObject="A56DEBB1-9B79-3B43-AF94-3B01A4745368" name="ACTIVE_USER_SCOPE_V">
<sourceConnName>appfsht-usm2</sourceConnName>
<sourceObjSchema>USM2</sourceObjSchema>
<sourceObjName>ACTIVE_USER_SCOPE_V</sourceObjName>
