<userDefinedSQL>CREATE OR REPLACE VIEW ACTIVE_USER_FEATURE_RESOURCE_V&lt;br/&gt;AS &lt;br/&gt;select distinct USER_NAME, APPLICATION_NAME, ROLE_NAME,&lt;br/&gt;			       ROLE_ID, SCOPE_ID, FEATURE_NAME, CATEGORY, DISCRIMINATOR&lt;br/&gt;			from (&lt;br/&gt;			  select uf.USER_NAME, uf.APPLICATION_NAME, uf.ROLE_NAME,&lt;br/&gt;			         uf.ROLE_ID, uf.SCOPE_ID, uf.FEATURE_NAME, d.CATEGORY, d.DISCRIMINATOR&lt;br/&gt;			  from ACTIVE_USER_FEATURE_V  uf,&lt;br/&gt;			       SCOPE_DATASET_T sd,&lt;br/&gt;			       DATASET_T d&lt;br/&gt;			  where sd.SCOPE_ID=uf.SCOPE_ID&lt;br/&gt;			    and d.DATASET_ID=sd.DATASET_ID&lt;br/&gt;			  union&lt;br/&gt;			  select uf.USER_NAME, uf.APPLICATION_NAME, uf.ROLE_NAME,&lt;br/&gt;			         uf.ROLE_ID, uf.SCOPE_ID, uf.FEATURE_NAME, NULL as CATEGORY, NULL as DISCRIMINATOR&lt;br/&gt;			  from ACTIVE_USER_FEATURE_V  uf&lt;br/&gt;			  where uf.SCOPE_ID is null&lt;br/&gt;			)</userDefinedSQL>
