<TableView class="oracle.dbtools.crest.model.design.relational.TableView" directorySegmentName="seg_0" id="9B25E28A-0518-942B-E7C0-384E5983A370" schemaObject="247D05E2-79CD-C825-54B3-5D5DE6D6B6B4" name="ACTIVE_USER_SCOPE_V">
