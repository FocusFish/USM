<TableView class="oracle.dbtools.crest.model.design.relational.TableView" directorySegmentName="seg_0" id="D9896715-D98F-D93E-580D-CA219B7BDC81" schemaObject="335F9972-90EC-227F-06DD-05E9B1A8D3B3" name="ACTIVE_USER_ROLE_Vv1">
