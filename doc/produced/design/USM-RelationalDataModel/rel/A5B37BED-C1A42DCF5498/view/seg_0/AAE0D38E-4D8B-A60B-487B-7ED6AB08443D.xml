<TableView class="oracle.dbtools.crest.model.design.relational.TableView" directorySegmentName="seg_0" id="AAE0D38E-4D8B-A60B-487B-7ED6AB08443D" schemaObject="335F9972-90EC-227F-06DD-05E9B1A8D3B3" name="USER_PROFILE_Vv1">
<createdBy>kroger</createdBy>
